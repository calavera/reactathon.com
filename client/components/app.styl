ks-normalize();
ks-box-sizing-reset();
ks-font-face("Roboto", "../../fonts/roboto/Roboto-Regular", normal, formats: truetype)

body
  width 100vw
  font-family "Roboto", helvetica, sans-serif
  background lighten($header-blue, 50%)

a
  color: black

.app
  $grid(column)
  min-height 100vh

.app-child
  $cell(12,, 0)
